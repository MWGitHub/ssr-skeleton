{
  "name": "ssr-skeleton",
  "description": "SSR Skeleton",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "build:client": "webpack --progress --config webpack.config.client.js",
    "watch:client": "npm run build:client -- --watch",
    "build:server": "webpack --progress --config webpack.config.server.js",
    "watch:server-bundle": "npm run build:server -- --watch",
    "watch:server": "nodemon dist/server.bundle.js --watch dist/server.bundle.js",
    "dev": "npm-run-all --parallel watch:server watch:server-bundle watch:client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MWGitHub/ssr-skeleton.git"
  },
  "keywords": [
    "radio"
  ],
  "author": "Michael Wang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MWGitHub/ssr-skeleton/issues"
  },
  "homepage": "https://github.com/MWGitHub/ssr-skeleton#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-async-functions": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "source-map-support": "^0.4.15",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "express": "^4.15.3",
    "prop-types": "^15.5.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
